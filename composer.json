{
  "name": "amritms/chuck-norris-jokes",
  "description": "Create random Chuck Norris jokes",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amrit Man Shrestha",
      "email": "amritms@gmail.com"
    }
